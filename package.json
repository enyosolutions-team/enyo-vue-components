{
  "name": "vue-aw-components",
  "version": "1.3.6",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "api": "json-server examples/data/db.js",
    "build:prod": "vue-cli-service build --mode production --no-clean --target lib --format es --name vec src/index.js --report",
    "dev": "vue-cli-service serve",
    "docs:build": "vuepress build docs",
    "docs:deploy": "scp -r docs/.vuepress/dist/* jenkins@develop:/apps/vue-aw-components.dev/dist/",
    "docs:dev": "vuepress dev docs",
    "start": "vue-cli-service serve"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "dependencies": {
    "dayjs": "^1.9.7",
    "localforage": "^1.9.0",
    "qs": "^6.6.0",
    "uuid": "^8.3.0"
  },
  "files": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@vue/cli": "^4.3.1",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^4.4.4",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.3.1",
    "chai": "^4.1.2",
    "chartist": "^0.11.0",
    "chartist-plugin-pointlabels": "^0.0.6",
    "chartist-plugin-tooltips": "^0.0.17",
    "core-js": "2",
    "crip-vue-loading": "^1.2.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "fecha": "^4.2.0",
    "husky": "^4.3.0",
    "jquery": "^3.5.1",
    "json-schema-test-data-generator": "https://github.com/enyosolutions-team/json-schema-test-data-generator",
    "json-schema-to-openapi-schema": "^0.4.0",
    "lint-staged": "^10.5.1",
    "lodash-webpack-plugin": "^0.11.5",
    "luxon": "^1.24.1",
    "marked": "^1.1.1",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "prettier-package-json": "^2.1.3",
    "sass-loader": "^7.1.0",
    "sweetalert2": "^8.2.6",
    "vue": "2.6.11",
    "vue-chartist": "2.2.1",
    "vue-clickaway": "^2.2.2",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-good-table": "^2.14.4",
    "vue-grid-layout": "^2.3.8",
    "vue-i18n": "^8.1.0",
    "vue-loader": "^15.7.0",
    "vue-loading-skeleton": "^1.1.9",
    "vue-multiselect": "^2.1.6",
    "vue-notifyjs": "^0.4.3",
    "vue-popperjs": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-select": "^3.10.3",
    "vue-template-compiler": "2.6.11",
    "vue2-daterange-picker": "^0.6.3",
    "vuedraggable": "^2.24.1",
    "vuejs-paginate": "^2.1.0",
    "vuepress": "^1.7.1",
    "vuex": "^3.1.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "weekstart": "^1.0.1"
  },
  "peerDependencies": {
    "axios": "^0.18.1",
    "bootstrap": "^4.3.1",
    "chartist": "^0.11.0",
    "chartist-plugin-pointlabels": "^0.0.6",
    "chartist-plugin-tooltips": "^0.0.17",
    "core-js": "^3.6.5",
    "crip-vue-loading": "^1.2.5",
    "fecha": "^4.2.0",
    "lodash": "^4.17.13",
    "luxon": "^1.24.1",
    "sweetalert2": "^8.2.6",
    "vue-base64-file-upload": "^1.0.4",
    "vue-chartist": "2.2.1",
    "vue-clickaway": "^2.2.2",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-good-table": "^2.14.4",
    "vue-grid-layout": "^2.3.8",
    "vue-i18n": "^8.1.0",
    "vue-loading-skeleton": "^1.1.9",
    "vue-multiselect": "^2.1.6",
    "vue-notifyjs": "^0.4.3",
    "vue-popperjs": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-select": "^3.10.3",
    "vue2-daterange-picker": "^0.6.3",
    "vuedraggable": "^2.24.1",
    "vuejs-paginate": "^2.1.0",
    "weekstart": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-reserved-keys": 0,
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "useTabs": false,
    "arrowParens": "always",
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.{js,vue}": "yarn lint --fix"
  }
}
